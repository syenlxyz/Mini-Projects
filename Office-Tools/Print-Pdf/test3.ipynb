   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "temp_path = Path.cwd() / 'temp.pdf'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "writer = PdfWriter()\n",
    "writer.add_blank_page(width=8.3*72, height=11.7*72)\n",
    "with open(temp_path, 'wb') as file:\n",
    "    writer.write(file)"
   ]
